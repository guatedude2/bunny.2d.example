{
  "fade-in": {
    "keys": [
      {
        "key": {
          "alpha": 0,
          "position.x": -50
        }
      }, {
        "key": {
          "alpha": 1,
          "position.x": 0
        },
        "easing": "easeOutQuad",
        "duration": 800
      }
    ]
  },
  "fade-out": {
    "keys": [
      {
        "key": {
          "alpha": 1
        }
      }, {
        "key": {
          "alpha": 0
        }
      }
    ]
  }
}